/* pages/book-time/book-time.wxss */

.culinarian-cell {
  padding: 10px;
  /* border-bottom: 1px solid gainsboro; */
  height: auto;
  width: auto;
  display: block;
}

.culinarian-cell .title2 {
  font-size: 14px;
  margin-bottom: 5px;
}

.culinarian-cell .header {
  position: relative;
  margin-bottom: 5px;
  border-bottom: 1px solid gainsboro;
  height: 40px;
}

.culinarian-cell .header .weui-btn {
  position: absolute;
  top: -12px;
  right: 10px;
}

.culinarian-cell .header .title {
  font-size: 20px;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  letter-spacing: 2px;
  line-height: 19px;
  padding: 8px 0;
}

.culinarian-cell .header .status {
  font-size: 20px;
  position: absolute;
  top: 1px;
  right: 100px;
}
